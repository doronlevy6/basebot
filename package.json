{
  "name": "base",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx run-many --target=test --all",
    "format": "prettier --write .",
    "lint": "nx run-many --target=lint --all --fix",
    "secrets:read": "aws s3 sync s3://base-service-secrets/secrets/basebot secrets",
    "secrets:write": "aws s3 sync secrets s3://base-service-secrets/secrets/basebot",
    "docker:login": "aws ecr get-login-password --region us-east-1 | docker login --username AWS --password-stdin 221762091942.dkr.ecr.us-east-1.amazonaws.com",
    "start:local": "npm run docker:login && docker compose up",
    "start:local2": "npm run docker:login && LOCAL_ENV=-2 docker compose up",
    "start:local:build": "npm run start:local -- --build",
    "start:local2:build": "npm run start:local2 -- --build",
    "clean:local": "docker compose down",
    "ngrok:gistbot": "concurrently npm:ngrok:gistbot:*",
    "ngrok:gistbot:main": "ngrok http 3000 --region=us --subdomain gistbot",
    "ngrok:gistbot:mailbot": "ngrok http 3004 --region=us --subdomain gistbot-mailbot",
    "ngrok:gistbot2": "concurrently npm:ngrok:gistbot2:*",
    "ngrok:gistbot2:main": "ngrok http 3000 --region=us --subdomain gistbot-2",
    "ngrok:gistbot2:mailbot": "ngrok http 3004 --region=us --subdomain gistbot-mailbot-2",
    "codegen:staging": "openapi-generator-cli generate -i https://stg.base.la/oapi-json -o libs/oapigen/src/lib -g typescript-axios --additional-properties=supportsES6=true,npmVersion=8.5.0",
    "codegen:local": "openapi-generator-cli generate -i http://localhost:3000/oapi-json -o libs/oapigen/src/lib -g typescript-axios --additional-properties=supportsES6=true,npmVersion=8.5.0",
    "fetch-session-data": "ts-node tools/extract_session_data.ts",
    "chatgist": "ts-node tools/chatgist/chatgist.ts",
    "stripesim:create-customer": "ts-node tools/stripesim/customer_created.ts",
    "stripesim:create-subscription": "ts-node tools/stripesim/customer_subscription_created.ts",
    "stripesim:update-subscription": "ts-node tools/stripesim/customer_subscription_updated.ts",
    "stripesim:delete-subscription": "ts-node tools/stripesim/customer_subscription_deleted.ts",
    "stripesim:invoice-paid": "ts-node tools/stripesim/invoice_paid.ts"
  },
  "private": true,
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@sendgrid/mail": "^7.7.0",
    "@slack/bolt": "^3.11.1",
    "@slack/web-api": "^6.7.1",
    "analytics-node": "^6.2.0",
    "aws-sdk": "^2.1250.0",
    "axios": "^0.27.2",
    "bullmq": "^3.6.5",
    "csv-parse": "^5.3.3",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.1",
    "emoji-js": "^3.7.0",
    "express": "^4.18.2",
    "googleapis": "^110.0.0",
    "ioredis": "^5.0.6",
    "knex": "^2.4.1",
    "nanoid": "^3.3.4",
    "niceware": "^4.0.0",
    "node-cron": "^3.0.2",
    "openai": "^3.0.1",
    "pg": "^8.7.3",
    "prom-client": "^14.0.1",
    "querystring": "^0.2.1",
    "stripe": "^10.16.0",
    "tslib": "^2.4.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@nrwl/cli": "14.1.9",
    "@nrwl/eslint-plugin-nx": "14.1.9",
    "@nrwl/express": "15.3.3",
    "@nrwl/jest": "14.1.9",
    "@nrwl/linter": "14.1.9",
    "@nrwl/node": "14.1.9",
    "@nrwl/workspace": "14.1.9",
    "@types/analytics-node": "^3.1.9",
    "@types/emoji-js": "^3.5.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.11.7",
    "@types/node-cron": "^3.0.5",
    "@types/validator": "^13.7.3",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "concurrently": "^7.6.0",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.5.1",
    "nx": "14.1.9",
    "prettier": "^2.5.1",
    "ts-jest": "27.1.4",
    "ts-node": "9.1.1",
    "typescript": "~4.6.2"
  }
}
